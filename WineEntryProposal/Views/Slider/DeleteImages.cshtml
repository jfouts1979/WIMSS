@{
    ViewBag.Title = "DeleteImages";
}

<h2>Select Image(s) To Delete:</h2>

@using (Html.BeginForm("DeleteImages", "Slider", FormMethod.Post))

{

<table style="background-color:azure" border="1">
    <thead style="background-color:Background">
    <tr>
    <th>Select</th>
    <th>Image</th>
    </tr>

    </thead>
    <tbody>
    @Html.EditorForModel()
    </tbody>



</table>
<br />
<input type="submit" value="Delete Selected Images" />
}
<br />  
<p>@Html.ActionLink("Slider Home", "Index", "Slider")</p>


